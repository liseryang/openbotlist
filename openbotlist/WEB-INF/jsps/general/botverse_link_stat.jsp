/** =============================================== */
/** == Link List Data, Only for the Link Section == */
/** == Bold and Dark Blue / Purple == */
/** =============================================== */
a.linklist_objlinks:link {
	font-family: "arial", verdana, helvetica, sans-serif;
	font-size: 11pt;
	font-weight: bold;
	margin-bottom: 0px;
	margin-top: 0px;
	background-color: transparent;
	color: #3838E5; 
	text-decoration:none;
}

/** Leave the visited link as darker than others */
a.linklist_objlinks:visited {
	font-family: "arial", verdana, helvetica, sans-serif;
	font-size: 11pt;
	font-weight: bold;
	margin-bottom: 0px;
	margin-top: 0px;
	color: #460C7B;
	background-color: transparent;
	text-decoration:none;
}

a.linklist_objlinks:hover {
	font-family: "arial", verdana, helvetica, sans-serif;
	font-size: 11pt;
	font-weight: bold;
	margin-bottom: 0px;
	margin-top: 0px;
	color: #3838E5;
	background-color: transparent;
	text-decoration:none;
}
a.linklist_objlinks:active {
